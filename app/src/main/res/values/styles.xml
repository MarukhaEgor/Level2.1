<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CustomButton" parent="">
        <item name="android:layout_width">match_parent</item>
        <item name="backgroundColor">@color/white</item>
        <item name="height">40dp</item>
        <item name="paddings">11dp</item>
        <item name="radius">10</item>
        <item name="srcCompat">@drawable/ic_google_login</item>
        <item name="label">GOOGLE</item>
    </style>

    <style name="settings_label_style">
        <item name="android:layout_marginStart">@dimen/main_paddings_16</item>
        <item name="android:layout_marginTop">@dimen/main_paddings_16</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/open_sans_light</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/txtSize_24</item>
    </style>

    <style name="profile_name_Style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/open_sans_light</item>
        <item name="android:textSize">@dimen/txtSize_18</item>
        <item name="android:fontProviderQuery">600</item>
    </style>

    <style name="profile_data_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/main_text_color</item>
        <item name="android:textSize">@dimen/txtSize_14</item>
        <item name="fontFamily">@font/open_sans_light</item>
    </style>

    <style name="main_btn_with_border_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/edit_btn_height</item>
        <item name="android:layout_marginStart">@dimen/main_paddings_16</item>
        <item name="android:layout_marginEnd">@dimen/main_paddings_16</item>
        <item name="android:layout_marginBottom">@dimen/main_paddings_16</item>
        <item name="android:background">@drawable/rnd_button_orange</item>
        <item name="android:letterSpacing">0.1</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="profile_activity_btn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/profile_activity_btn_height</item>
        <item name="android:letterSpacing">0.1</item>
        <item name="android:layout_marginStart">@dimen/main_paddings_16</item>
        <item name="android:layout_marginEnd">@dimen/main_paddings_16</item>
        <item name="android:layout_marginBottom">@dimen/main_paddings_16</item>
        <item name="android:background">@drawable/rnd_button</item>
        <item name="android:backgroundTint">@color/main_orange</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="profile_soc_btn">
        <item name="android:layout_width">@dimen/rnd_social_btn_size</item>
        <item name="android:layout_height">@dimen/rnd_social_btn_size</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="elevation">0dp</item>
        <item name="backgroundTint">@color/main_orange</item>
        <item name="borderWidth">@dimen/border_with</item>
        <item name="fabCustomSize">@dimen/rnd_social_btn_size</item>
        <item name="tint">@color/main_orange</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="profile_avatar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="editText_register" parent="TextAppearance.AppCompat">
        <item name="hintTextColor">@color/main_text_color</item>
        <item name="boxStrokeWidth">1dp</item>
        <item name="boxStrokeWidthFocused">1dp</item>
        <item name="expandedHintEnabled">false</item>
        <item name="boxStrokeColor">@color/main_text_color</item>
        <item name="android:textSize">@dimen/txtSize_14</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColorHint">@color/main_text_color</item>
        <item name="android:paddingStart">@dimen/editText_register_padding_start</item>
    </style>

    <style name="signUp_lets_get_textView">
        <item name="android:textSize">@dimen/txtSize_24</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">"@font/open_sans_light"</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
    </style>

</resources>